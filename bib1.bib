	journal = {Applied Thermal Engineering},
	keywords = {Catalytic methane combustion, Catalytic methane reforming, computational fluid dynamics (CFD), Catalyst segmentation, Hydrogen production optimization, Multi-objective genetic algorithm},
	pages = {119140},
	title = {Novel design and multi-objective optimization of autothermal steam methane reformer to enhance hydrogen production and thermal matching},
	url = {https://www.sciencedirect.com/science/article/pii/S1359431122010717},
	volume = {217},
	year = {2022},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S1359431122010717},
