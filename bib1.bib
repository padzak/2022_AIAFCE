@article{Simson2009,
	abstract = {The reforming of pure ethanol was studied over a bi-metallic precious metal (Rh/Pt) catalyst deposited on a ceramic monolith in order to analyze reforming process conditions. High ethanol conversion tests performed at low space velocities (<20,000hâˆ’1) confirmed that the catalyst could achieve 100% ethanol conversion to equilibrium concentrations of H2, CO, CO2 and CH4. Low conversion tests at high space velocities (â‰¥50,000hâˆ’1) were conducted to produce an overall rate expression with an activation energy of 85kJmoleâˆ’1. The reaction was found to have a 1.2 reaction order for ethanol and zero order for water for stoichiometric ethanol and water ratios. In addition, the impact of non-catalytic reactions was studied. The results showed that the catalyst was capable of reforming ethanol as well as the by-products from non-catalytic reactions at 500--700$\,^{\circ}$C. This work is part 1 of a series to develop a process for steam reforming E85 (85% ethanol+15% gasoline) to generate hydrogen for a fuel cell.},
	author = {Simson, A. and Waterman, E. and Farrauto, R. and Castaldi, M.},
