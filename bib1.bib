	date-added = {2021-12-19 14:08:04 +0100},
	date-modified = {2022-09-29 19:38:20 +0200},
	doi = {https://doi.org/10.1016/j.ijhydene.2020.12.183},
	issn = {0360-3199},
	journal = {International Journal of Hydrogen Energy},
	keywords = {Methanol steam reforming, Hydrogen production, Metal foam, Segmented catalyst, Metal foam as support of catalyst},
