	doi = {10.1007/s00231-018-2331-5},
	id = {Mozdzierz2018},
	isbn = {1432-1181},
	journal = {Heat and Mass Transfer},
	pages = {2331--2341},
	title = {An afterburner-powered methane/steam reformer for a solid oxide fuel cells application},
	ty = {JOUR},
	volume = {54},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1007/s00231-018-2331-5}}

@article{Komatsu2009,
	abstract = {This paper presents a performance analysis for a Solid Oxide Fuel Cell (SOFC) - Micro Gas Turbine (MGT) hybrid power system using biogas. It was assumed that the supplied biogas consisted of CH4 and CO2; the performance was predicted comprehensively while the concentrations of the both components were varied. The concentration of CH4 in biogas was varied from 100 [%] to 20[%]. The obtained results figured the system performance degradation with decreasing CH4 concentration. It also suggested designing the cell active area to fulfil the requirements for an electrical capacity of 220 [kW]. It was found that lager cell active area was required while the CH4 concentration diluted. The results are promising for the future use of biogas in SOFC-MGT hybrid systems.},
	author = {Komatsu, Y. and Kimijima, S. and Szmyd, J. S.},
