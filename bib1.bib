	journal = {Energy},
	keywords = {Combustion intensifier, Pollutant emission reduction, Wood pellets, Agro pellets, Subsequent combustion},
	pages = {120929},
	title = {Numerical and experimental methods in development of the novel biomass combustion system concept for wood and agro pellets},
	volume = {231},
	year = {2021},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0360544221011774},
