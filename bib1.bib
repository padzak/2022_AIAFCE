	issn = {0378-7753},
	journal = {Journal of Power Sources},
	keywords = {Direct internal reforming, Solid oxide fuel cell, Steam methane reforming, Mathematical modeling},
	pages = {230857},
	title = {A review on mathematical modelling of Direct Internal Reforming- Solid Oxide Fuel Cells},
	url = {https://www.sciencedirect.com/science/article/pii/S0378775321013446},
	volume = {520},
	year = {2022},
